{
  "title": "Custom volume, play or pause controls",
  "rules": [
    {
      "description": "Change fn +a  to be volume_decrement",
      "manipulators": [
        {
          "type": "basic",
          "from": {
"key_code": "a",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "volume_decrement"
            }
          ]
        }
      ]
    },
    {
      "description": "Change fn + s to be volume up",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "volume_increment"
            }
          ]
        }
      ]
    },
    {
      "description": "Change fn + c to be play/pause",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "play_or_pause"
            }
          ]
        }
      ]
    },
    {
      "description": "Change fn + x to be play/pause",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "x",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "fastforward"
            }
          ]
        }
      ]
    },
    {
      "description": "Change fn + z to be play/pause",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": [
                "fn"
              ]
            }
          },
          "to": [
            {
              "key_code": "rewind"
            }
          ]
        }
      ]
    }
  ]
}
