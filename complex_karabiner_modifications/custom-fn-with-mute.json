{
  "title": "Change fn to mute and fn",
  "rules": [
	{
	  "description": "Post mute if fn is tapped, fn if held.",
	  "manipulators": [
        {
    "type": "basic",
    "from": {
        "key_code": "fn",
        "modifiers": {
            "optional": [
                "any"
            ]
        }
    },
    "to": [
        {
            "key_code": "fn",
            "lazy": true
        }
    ],
    "to_if_alone": [
        {
            "key_code": "mute"
        }
    ]
}
	  ]
	}
  ]
}
